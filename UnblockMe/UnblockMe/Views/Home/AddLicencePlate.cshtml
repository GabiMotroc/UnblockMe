@{
    ViewData["Title"] = "Add Licence Plate";
}

@functions 
{
    public bool checkPrime(int number)
    {
        if (number <= 1)
            return false;
        if (number == 2)
            return true;
        if (number % 2 == 0)
            return false;
        for (int i = 3; i < number / 2 + 1; i += 2)
            if (number % i == 0)
                return false;
        return true;
    }
}
<h3>Acesta e un test.</h3>

@for(int i = 0; i < 100; i++)
{
    if (checkPrime(i))
    {
        <b>@i<br></b>
    }
    else
    {
        <body>@i<br></body>
    }
}
